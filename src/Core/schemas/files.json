{
    "resource": "filemanager/api",
    "methods": {
        "upload": {
            "path": "v3/files/upload",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/v3/upload_new_file",
            "action": "createOrUpdate",
            "contentType": "multipart/form-data"
        },
        "replace": {
            "path": "v3/files/{file_id}/replace",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/v3/upload_replacement_file",
            "action": "createOrUpdate"
        },
        "getAll": {
            "path": "v2/files",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/get_files",
            "action": "get"
        },
        "get": {
            "path": "v2/files/{file_id}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/get_files_file_id",
            "action": "get"
        },
        "changeAccess": {
            "path": "v3/files/set-access/batch",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/update_file_access",
            "action": "createOrUpdate"
        },
        "delete": {
            "path": "v2/files/{file_id}/full-delete",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/hard_delete_file_and_associated_objects",
            "action": "createOrUpdate"
        },
        "getPublicUrl": {
            "path": "v3/files/{fileId}/signed-url",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/get-signed-url-private-file",
            "action": "get"
        },
        "move": {
            "path": "v2/files/{file_id}/move-file",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/post_files_file_id_move_file",
            "action": "createOrUpdate"
        },
        "getAllFolders": {
            "path": "v2/folders",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/get_folders",
            "action": "get"
        },
        "getFolder": {
            "path": "v2/folders/{folder_id}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/get_folders_folder_id",
            "action": "get"
        },
        "createFolder": {
            "path": "v2/folders",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/post_folders",
            "action": "createOrUpdate"
        },
        "moveFolder": {
            "path": "v2/folders/{folder_id}/move-folder",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/post_folders_folder_id_move_folder",
            "action": "createOrUpdate"
        },
        "deleteFolder": {
            "path": "v2/folders/{folder_id}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/files/delete_folders_folder_id",
            "action": "delete"
        }
    }
}